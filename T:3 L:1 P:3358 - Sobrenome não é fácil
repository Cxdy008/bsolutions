using System; 

class URI {

    static void Main(string[] args) { 

        int N = int.Parse(Console.ReadLine());
        
        for(int i = 0; i<N;i++){
            string sobrenome = Console.ReadLine();
            char[] vogais = {'a','e','i','o','u'};
            int consoantesConsecutivas = 0;
            bool  contemVogal = false;
            for(int j = 0; j<sobrenome.Length;j++){
                for(int k = 0; k<vogais.Length;k++){
                    if(sobrenome.ToLower()[j] == vogais[k]){
                       contemVogal = true;
                       break;
                    }
                    else{
                        contemVogal = false;
                    }
                    
                }
                if(!contemVogal){
                    consoantesConsecutivas++;
                    if(consoantesConsecutivas>=3)
                        break;                    
                }
                else{
                    consoantesConsecutivas = 0;
                }
                
            }
            if(consoantesConsecutivas<3){
                Console.WriteLine("{0} eh facil",sobrenome);

            }
            else{
                Console.WriteLine("{0} nao eh facil",sobrenome);
            }
            
            
        }

    }

}
