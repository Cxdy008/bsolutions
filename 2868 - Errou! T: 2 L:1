int C = int.Parse(Console.ReadLine());
        int respostaDada, respostaCerta=0, num1, num2, numDeRs;
        char  tipoOperacao;
        string operacao, Rs="";
        string [] divOperacao;
        for(int i = 0; i<C;i++){
            operacao = Console.ReadLine();
            divOperacao = operacao.Split(' ');
            num1 = int.Parse(divOperacao[0]);
            tipoOperacao = char.Parse(divOperacao[1]);
            num2 = int.Parse(divOperacao[2]);
            respostaDada = int.Parse(divOperacao[4]);
            switch(tipoOperacao){
                case '+':
                    respostaCerta = num1 + num2;
                    break;
                case '-':
                    respostaCerta = num1 - num2;
                    break;
                case '/':
                    respostaCerta = num1 / num2;
                    break;
                case 'x':
                    respostaCerta = num1 * num2;
                    break;
            }
            numDeRs = respostaDada - respostaCerta;
            numDeRs = numDeRs>0?numDeRs:numDeRs*-1;
            
            for(int j=0;j<numDeRs;j++){
                Rs+="r";
            }
            Console.WriteLine("E"+Rs+"ou!");
            Rs="";
